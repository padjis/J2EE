import java.util.*;

import javax.persistence.Entity;
import javax.persistence.Id;

import com.indev.worlcup.extra.Freelancerequest;

@Entity
/** @pdOid 5f5c842e-cb1b-4706-9164-5a4d6c3b2b10 */
public class Users {
	@Id
   /** @pdOid 5f2c9950-255b-48ee-8f59-31a7eefcecc5 */
   private String username;
   /** @pdOid 1a22e978-e1fa-405e-9328-55565724b825 */
   private String firstname;
   /** @pdOid be2c02c9-6544-4ef0-8497-1b80b7237b72 */
   private String lastname;
   /** @pdOid 7bffc176-8ec7-4e5d-8f40-417d66ac5edc */
   private String password;
   /** @pdOid c6b66280-ef04-4879-8859-91a61f177901 */
   private String privilige;
   /** @pdOid 45523a9f-c5b3-49ec-b429-755a9a4c9f72 */
   private String mail;
   /** @pdOid 76b458e0-e6fe-4b51-a0b1-1a949e7d5324 */
   private int numtel;
   /** @pdOid 447bf5e8-04f0-44b3-aa64-3480a4a0695e */
   private String enterprisename;
   /** @pdOid 5c78cea2-79f8-414a-a628-7fdc677ef2e8 */
   private int enterprisetel;
   /** @pdOid a8064a17-40ae-43e3-83cf-1adba26006b6 */
   private String path;
   /** @pdOid 3f1d81a4-2da4-4af9-a0ba-0130e4fef199 */
   private int state;
   
   
   public String getUsername() {
	return username;
}

public void setUsername(String username) {
	this.username = username;
}

public String getFirstname() {
	return firstname;
}

public void setFirstname(String firstname) {
	this.firstname = firstname;
}

public String getLastname() {
	return lastname;
}

public void setLastname(String lastname) {
	this.lastname = lastname;
}

public String getPassword() {
	return password;
}

public void setPassword(String password) {
	this.password = password;
}

public String getPrivilige() {
	return privilige;
}

public void setPrivilige(String privilige) {
	this.privilige = privilige;
}

public String getMail() {
	return mail;
}

public void setMail(String mail) {
	this.mail = mail;
}

public int getNumtel() {
	return numtel;
}

public void setNumtel(int numtel) {
	this.numtel = numtel;
}

public String getEnterprisename() {
	return enterprisename;
}

public void setEnterprisename(String enterprisename) {
	this.enterprisename = enterprisename;
}

public int getEnterprisetel() {
	return enterprisetel;
}

public void setEnterprisetel(int enterprisetel) {
	this.enterprisetel = enterprisetel;
}

public String getPath() {
	return path;
}

public void setPath(String path) {
	this.path = path;
}

public int getState() {
	return state;
}

public void setState(int state) {
	this.state = state;
}


}