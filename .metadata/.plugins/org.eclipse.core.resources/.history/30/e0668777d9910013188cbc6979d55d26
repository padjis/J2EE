import java.util.*;

import javax.persistence.Entity;
import javax.persistence.Id;


@Entity
public class Player {
	@Id
   private int idplayer;
   private String namep;
   private int agep;
   private int butp;
   private int carrp;
   private int carjp;
   private int postep;
   private int butep;
   private int pathp;
   private int textp;
   public int getIdplayer() {
	return idplayer;
}

public void setIdplayer(int idplayer) {
	this.idplayer = idplayer;
}

public String getNamep() {
	return namep;
}

public void setNamep(String namep) {
	this.namep = namep;
}

public int getAgep() {
	return agep;
}

public void setAgep(int agep) {
	this.agep = agep;
}

public int getButp() {
	return butp;
}

public void setButp(int butp) {
	this.butp = butp;
}

public int getCarrp() {
	return carrp;
}

public void setCarrp(int carrp) {
	this.carrp = carrp;
}

public int getCarjp() {
	return carjp;
}

public void setCarjp(int carjp) {
	this.carjp = carjp;
}

public int getPostep() {
	return postep;
}

public void setPostep(int postep) {
	this.postep = postep;
}

public int getButep() {
	return butep;
}

public void setButep(int butep) {
	this.butep = butep;
}

public int getPathp() {
	return pathp;
}

public void setPathp(int pathp) {
	this.pathp = pathp;
}

public int getTextp() {
	return textp;
}

public void setTextp(int textp) {
	this.textp = textp;
}

public int getMatchjp() {
	return matchjp;
}

public void setMatchjp(int matchjp) {
	this.matchjp = matchjp;
}

public int getMatchgp() {
	return matchgp;
}

public void setMatchgp(int matchgp) {
	this.matchgp = matchgp;
}

public Team getTeam() {
	return team;
}

public void setTeam(Team team) {
	this.team = team;
}

